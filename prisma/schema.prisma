generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum UserRole {
    ADMIN
    USER
    SUPERADMIN
}

model User{
    id Int @id @default(autoincrement())
    name String
    email String @unique
    age Int
    password String
    role UserRole @default(USER)

    @@map("users")
}